<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <meta name="robots" content="noindex,nofollow" />
        <meta content='width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no' name='viewport'>
        <title>{% block title %}{% endblock %}</title>

        {% block head %}{% endblock %}

        <link href="//maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
        <link href="//cdnjs.cloudflare.com/ajax/libs/font-awesome/4.1.0/css/font-awesome.min.css" rel="stylesheet" type="text/css" />
        <!-- Ionicons -->
        <link href="//code.ionicframework.com/ionicons/1.5.2/css/ionicons.min.css" rel="stylesheet" type="text/css" />

        <!-- Deeson fonts -->
        <link rel="stylesheet" href="{{ asset('bundles/deesonwarden/assets/fonts/icomoon/style.css') }}" />
        <!-- Morris chart -->
        <link rel="stylesheet" href="{{ asset('bundles/deesonwarden/assets/css/morris/morris.css') }}" />
        <!-- jvectormap -->
        <link rel="stylesheet" href="{{ asset('bundles/deesonwarden/assets/css/datepicker/datepicker3.css/style.css') }}" />
        <!-- Date Picker -->
        <link rel="stylesheet" href="{{ asset('bundles/deesonwarden/assets/fonts/icomoon/style.css') }}" />
        <!-- Daterange picker -->
        <link rel="stylesheet" href="{{ asset('bundles/deesonwarden/assets/css/daterangepicker/daterangepicker-bs3.css') }}" />
        <!-- bootstrap wysihtml5 - text editor -->
        <link rel="stylesheet" href="{{ asset('bundles/deesonwarden/assets/css/bootstrap-wysihtml5/bootstrap3-wysihtml5.min.css') }}" />
        <!-- Theme style -->
        <link rel="stylesheet" href="{{ asset('bundles/deesonwarden/assets/css/AdminLTE.css') }}" />

        <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
        <![endif]-->
        <style>
            .bg-brand {
                background: #f10008!important;
                color: white;
            }
        </style>
    </head>
    <body class="skin-blue fixed">

        {% block content %}{% endblock %}

        <script src="//ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js"></script>
        <script src="//maxcdn.bootstrapcdn.com/bootstrap/3.2.0/js/bootstrap.min.js" type="text/javascript"></script>
        <script src="//code.jquery.com/ui/1.11.1/jquery-ui.min.js" type="text/javascript"></script>
        <!-- Morris.js charts -->
        <script src="//cdnjs.cloudflare.com/ajax/libs/raphael/2.1.0/raphael-min.js"></script>
        <script src="{{ asset('bundles/deesonwarden/assets/js/plugins/morris/morris.min.js') }}" type="text/javascript"></script>
        <!-- Sparkline -->
        <script src="{{ asset('bundles/deesonwarden/assets/js/plugins/sparkline/jquery.sparkline.min.js') }}" type="text/javascript"></script>
        <!-- jvectormap -->
        <script src="{{ asset('bundles/deesonwarden/assets/js/plugins/jvectormap/jquery-jvectormap-1.2.2.min.js') }}" type="text/javascript"></script>
        <script src="{{ asset('bundles/deesonwarden/assets/js/plugins/jvectormap/jquery-jvectormap-world-mill-en.js') }}" type="text/javascript"></script>
        <!-- jQuery Knob Chart -->
        <script src="{{ asset('bundles/deesonwarden/assets/js/plugins/jqueryKnob/jquery.knob.js') }}" type="text/javascript"></script>
        <!-- daterangepicker -->
        <script src="{{ asset('bundles/deesonwarden/assets/js/plugins/daterangepicker/daterangepicker.js') }}" type="text/javascript"></script>
        <!-- datepicker -->
        <script src="{{ asset('bundles/deesonwarden/assets/js/plugins/datepicker/bootstrap-datepicker.js') }}" type="text/javascript"></script>
        <!-- Bootstrap WYSIHTML5 -->
        <script src="{{ asset('bundles/deesonwarden/assets/js/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.all.min.js') }}" type="text/javascript"></script>
        <!-- iCheck -->
        <script src="{{ asset('bundles/deesonwarden/assets/js/plugins/iCheck/icheck.min.js') }}" type="text/javascript"></script>

        <script src="//ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js"></script>
        <script src="//maxcdn.bootstrapcdn.com/bootstrap/3.2.0/js/bootstrap.min.js" type="text/javascript"></script>
        <!-- DATA TABES SCRIPT -->
        <script src="{{ asset('bundles/deesonwarden/assets/js/plugins/datatables/jquery.dataTables.js') }}" type="text/javascript"></script>
        <script src="{{ asset('bundles/deesonwarden/assets/js/plugins/datatables/dataTables.bootstrap.js') }}" type="text/javascript"></script>
        <!-- AdminLTE App -->
        <script src="{{ asset('bundles/deesonwarden/assets/js/AdminLTE/app.js') }}" type="text/javascript"></script>
        <!-- page script -->
        <script type="text/javascript">
            $(function() {
                $("#example1").dataTable();
                $('#example2').dataTable({
                    "bPaginate": true,
                    "bLengthChange": false,
                    "bFilter": false,
                    "bSort": true,
                    "bInfo": true,
                    "bAutoWidth": false
                });
            });
        </script>

    </body>
</html>
